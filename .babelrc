{
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-react", { "development": true }],
        "@babel/preset-typescript"
      ]
    },
    "test": {
      "presets": ["@babel/env"]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel"
  ],
  "presets": [
    ["@babel/env", { "modules": false }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ]
}