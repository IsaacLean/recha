{
  "name": "recha",
  "version": "0.0.1",
  "description": "",
  "main": "build/production/webpack/main.js",
  "productName": "Recha",
  "build": {
    "appId": "com.lean.recha",
    "directories": {
      "output": "build/production/electron-builder"
    },
    "files": [
      "./build/production/webpack/**/*"
    ]
  },
  "scripts": {
    "build": "webpack --env production",
    "build:debug": "node --inspect-brk node_modules/.bin/webpack --env production",
    "build:dev": "webpack --env development",
    "build:dev:debug": "node --inspect-brk node_modules/.bin/webpack --env development",
    "build:dev:stats": "npm run build:dev -- --json > stats.development.json",
    "build:stats": "npm run build -- --json > stats.production.json",
    "dist": "npm run build && electron-builder",
    "dist:debug": "DEBUG=electron-builder npm run dist",
    "format": "prettier --write --parser=typescript --print-width=120 --semi=false --single-quote \"{config,src}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "npm run lint:js",
    "lint:js": "eslint --cache \"{config,src}/**/*.{js,jsx,ts,tsx}\"",
    "lint:styles": "stylelint src/**/*.scss src/**/*.css",
    "main:build": "webpack --env production --config config/main/index.js",
    "main:build:debug": "node --inspect-brk node_modules/.bin/webpack --env production --config config/main/index.js",
    "main:build:dev": "webpack --env development --config config/main/index.js",
    "main:build:dev:debug": "node --inspect-brk node_modules/.bin/webpack --env development --config config/main/index.js",
    "main:build:dev:stats": "npm run main:build:dev -- --json > stats.main.development.json",
    "main:build:stats": "npm run main:build -- --json > stats.main.production.json",
    "main:start": "electron build/production/webpack/main.js",
    "main:start:debug": "electron --inspect-brk build/production/webpack/main.js",
    "main:start:dev": "electron build/development/main/main.js",
    "main:start:dev:debug": "electron --inspect-brk build/development/main/main.js",
    "main:start:dev:watch": "nodemon --watch nodemon.json --watch webpack.config.js --watch config/common --watch config/main --watch src/main --exec \"npm run main:build:dev && npm run main:start:dev\"",
    "main:start:dev:watch:debug": "nodemon --watch nodemon.json --watch webpack.config.js --watch config/common --watch config/main --watch src/main --exec \"npm run main:build:dev && npm run main:start:dev:debug\"",
    "pack": "npm run build && electron-builder --dir",
    "rend:build": "webpack --env production --config config/renderer/index.js",
    "rend:build:debug": "node --inspect-brk node_modules/.bin/webpack --env production --config config/renderer/index.js",
    "rend:build:dev": "webpack --env development --config config/renderer/index.js",
    "rend:build:dev:debug": "node --inspect-brk node_modules/.bin/webpack --env development --config config/renderer/index.js",
    "rend:build:dev:stats": "npm run rend:build:dev -- --json > stats.rend.development.json",
    "rend:build:stats": "npm run rend:build -- --json > stats.rend.production.json",
    "rend:start": "http-server ./build/production/webpack -p 9090",
    "rend:start:dev": "http-server ./build/development/renderer -p 8080",
    "rend:start:dev:watch": "nodemon --watch nodemon.json ---watch webpack.config.js --watch config/common --watch config/renderer --exec \"webpack-dev-server --env development --config config/renderer/index.js\"",
    "start": "npm run main:start",
    "start:debug": "npm run main:start:debug",
    "start:dev": "npm run main:start:dev",
    "start:dev:debug": "npm run main:start:dev:debug",
    "start:dev:watch": "npm run main:start:dev:watch",
    "start:dev:watch:debug": "npm run main:start:dev:watch:debug",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsaacLean/recha.git"
  },
  "author": "Matthew Lean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IsaacLean/recha/issues"
  },
  "homepage": "https://github.com/IsaacLean/recha#readme",
  "dependencies": {
    "@material/react-button": "^0.9.2",
    "@material/react-card": "^0.9.0",
    "@material/react-checkbox": "^0.9.0",
    "@material/react-drawer": "^0.8.0",
    "@material/react-layout-grid": "^0.8.0",
    "@material/react-list": "^0.9.0",
    "@material/react-material-icon": "^0.9.0",
    "@material/react-text-field": "^0.9.0",
    "@material/react-top-app-bar": "^0.9.0",
    "@material/react-typography": "^0.8.0",
    "cross-fetch": "^3.0.0",
    "electron": "^4.0.3",
    "normalizr": "^3.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-beautiful-dnd": "^10.0.4",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.5",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/classnames": "^2.2.7",
    "@types/node": "^10.12.21",
    "@types/react": "^16.7.21",
    "@types/react-beautiful-dnd": "^10.0.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^1.1.1",
    "@typescript-eslint/parser": "^1.1.1",
    "autoprefixer": "^9.4.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-preset-airbnb": "^3.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "deep-freeze": "0.0.1",
    "devtron": "^1.4.0",
    "electron-builder": "^20.38.5",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.2.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^24.0.0",
    "jsdom": "^13.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.3",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react-test-renderer": "^16.7.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
